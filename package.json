{
  "name": "transbank-pos-sdk",
  "version": "5.0.0",
  "description": "SDK Web para POS Integrado en Node.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "example": "tsx ./examples/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "POS",
    "transbank",
    "serial"
  ],
  "author": {
    "name": "Transbank Developers",
    "email": "transbankdevelopers@continuum.cl"
  },
  "contributors": [
    { "name": "Gonzalo De SpÃ­rito" }
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@serialport/parser-inter-byte-timeout": "^13.0.0",
    "lrc-calculator": "^0.1.1",
    "serialport": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "inquirer": "^13.0.2",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
